    "description": "Phprest Sample Heroku App.",
    "keywords": ["phprest", "rest", "restful", "framework", "heroku"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Adam Balogh",
            "email": "baloghbh@gmail.com"
        }
    ],
    "require": {
        "phprest/phprest": "dev-master",
